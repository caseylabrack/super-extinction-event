version 42
__lua__
index=1

scenarios={
"brontosaurus",
"oviraptor",
"spinosaurus",
--"lizard thing (todo)",
}

function _init()
	cartdata("caseylabrack_superdino")
end

function _update()
	
	if btnp(⬇️) then
		index+=1
	end
	
	if btnp(⬆️) then
		index-=1
	end

	if index>#scenarios then index=1 end
	if index<1 then index=#scenarios end

	if btnp(❎) then
		load("super-extinction-event",
		"back to menu",
		index)
	end

--	if btnp(🅾️) then
--		load("super-extinction-event","menu","1")
--	end
--	if btnp(❎) then
--		load("super-extinction-event","menu","2")
--	end
end

function _draw()
	cls(1)
	
	ypos=10
	for num,val in ipairs(scenarios) do
		if num==index then
			print("◆",14,ypos,rnd({13,14}))
		end
		print(val,20,ypos,11)
		ypos+=10
	end
end
