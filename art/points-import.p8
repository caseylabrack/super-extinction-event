--art helper cart
cb=""
tx,ty=0,0
r=0
points={}

function _init()
	
end

function _update()
	local cb=stat(4)
	if cb~="" then
		msg=cb
		
		input=split(cb," ")
		
		--validate input somewhat
		valid=true
		for token in ipairs(input) do
			if type(token)~="number" then
				valid=false
				break
			end
		end		
		if valid then points=input end
	end
	
	if btnp(⬅️) then tx-=1 end
	if btnp(➡️) then tx+=1 end
	if btnp(⬆️) then ty+=1 end
	if btnp(⬇️) then ty-=1 end
	if btn(❎) then r+=.005 end
	if btn(🅾️) then
		local finalpts=translate(points,tx,ty)
		local export=""
		for pt in all(finalpts) do
			export..=pt..","
		end
		--remove trailing comma
		export=sub(export,1,#export-1)
		printh(export,'@clip')
		extcmd("shutdown")
	end	
end

function _draw()
	cls()
	camera(-64,-64)
	circ(0,0,3,14)
	if btn(❎) then
		z=translate(points,tx,ty)
		z=rotate(z,r)
	else
		z=translate(points,tx,ty)
	end
	renderpoly(z)
	camera()
	print("translated: "..tx..", "..ty)
end
